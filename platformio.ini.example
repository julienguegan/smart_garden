[env:featheresp32]
platform = espressif32
board = featheresp32
framework = arduino
lib_deps = blynkkk/Blynk@^1.1.0
build_flags = 
    -D WIFI_PASSWORD=\"your_wifi_password_here\"

; Pour utiliser ce fichier :
; 1. Copiez ce fichier et renommez-le en 'platformio.ini'
; 2. Remplacez 'your_wifi_password_here' par votre mot de passe WiFi
; 3. OU d√©finissez la variable d'environnement WIFI_PASSWORD avant de compiler :
;    export WIFI_PASSWORD="votre_mot_de_passe"
